using namespace std;

int main()
{
	char data[100];
	int a,b, res;
	char op;
	ifstream input;
	ofstream output;
	input.open("input.dat");
	input.open("output.dat");	
	cout<<"Entering parse"<<endl;
	while(!input.eof())
	{	input>>data;
		cout<<"operation: "<<data<<endl;
		if(!strcmp(data, "ADD")){
			op = '+';
		}
