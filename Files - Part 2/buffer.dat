	int a,b, res;
	char op;
	ifstream input;
	ofstream output;
	input.open("input.dat");
	output.open("output.dat");
	while(!input.eof())
	{	input>>operand;
		if(!strcmp(data, "ADD")){
			op = '+';
		}
		else if(!strcmp(data, "SUB")){
			op = '-';
		}
		
		else if(!strcmp(data, "MUL")){
			op = '*';
		}
		else if(!strcmp(data, "DIV")){
			op = '/';
		}
		input>>data;
		a = atoi(data);
		input>>data;
		b = atoi(data);
		switch(op){
			case '+' : res = a+b;
				break;
			case '-' : res = a-b;
				break;
			case '*' : res = a*b;
				break;
			case '/' : res = a/b;
				break;
		}
		output<<res<<endl;
	}
	input.close();
}


